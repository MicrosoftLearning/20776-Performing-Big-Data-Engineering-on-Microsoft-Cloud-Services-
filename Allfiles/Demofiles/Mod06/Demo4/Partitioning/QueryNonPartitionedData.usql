@salesData = 
    SELECT State,
           ProductID,
           NumSold,
           PricePerItem
    FROM SalesData.dbo.OrdinarySales;

@nySales =
    SELECT ProductID,
           SUM(NumSold * PricePerItem) AS TotalSalesValue
    FROM @salesData
    WHERE State == "NY"
    GROUP BY ProductID;

OUTPUT @nySales
    TO "/NYOrdinarySales.csv"
    USING Outputters.Csv(outputHeader: true, quoting: false);